<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>switch.ac</path>

  <animation>
    <object-name>switch</object-name>
    <object-name>tour</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>switch</object-name>
    <object-name>tour</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>switch</object-name>
    <object-name>tour</object-name>
    <condition>
      <property>controls/electric/battery-switch</property>
    </condition>
    <emission>
      <factor-prop>controls/lighting/luminosite-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.40 </green>
      <blue>  0.30 </blue>
    </emission>
  </animation>

</PropertyList>
